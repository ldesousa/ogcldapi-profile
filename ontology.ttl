@prefix dcterms: <http://purl.org/dc/terms/> .
PREFIX geo: <http://www.opengis.net/ont/geosparql#> 
PREFIX ogcld: <https://data.surroundaustralia.com/def/ogcldapi/>
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<https://data.surroundaustralia.com/def/ogcldapi/ont>
    a owl:Ontology ;
.

ogcld:Dataset 
    a owl:Class ;
    skos:prefLabel "Dataset"@en ;
    skos:definition "A collection of spatial feature data, arranged in one or more collections"@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dcterms:hasPart ;
        owl:class ogcld:FeatureCollection ;
    ] ;    
.

ogcld:FeatureCollection 
    a owl:Class ;
    skos:prefLabel "Feature Collection"@en ;
    skos:definition "A collection of spatial Features"@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty dcterms:isPartOf ;
        owl:class ogcld:Dataset ;
    ] ;
